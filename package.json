{
  "name": "dcl-ecs-quests",
  "version": "1.0.0",
  "description": "Decentraland Library for creating quests ECS components to show in UI",
  "scripts": {
    "start": "dcl start",
    "build": "build-ecs",
    "watch": "build-ecs --watch",
    "deploy:now": "dcl export && now export",
