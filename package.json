    "ecs:install-next": "npm install --save-dev decentraland-ecs@next"
  },
  "decentralandLibrary": {},
  "main": "./dist/dcl-ecs-quests.js",
  "typings": "./dist/dcl-ecs-quests.d.ts",
  "devDependencies": {
    "decentraland-ecs": "latest"
  }
}
